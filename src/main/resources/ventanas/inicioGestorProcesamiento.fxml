<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="520.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.uniquindio.plataforma.controladores.inicioGestorProcesamientoControlador">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="193.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagenes/UQINICIO.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="122.0" prefHeight="335.0" prefWidth="485.0" spacing="10" xmlns:fx="http://javafx.com/fxml">
         <ComboBox fx:id="comboBox" promptText="Selecciona un elemento" />
         <Button fx:id="procesarNoticias" mnemonicParsing="false" onAction="#convertirNoticias" prefHeight="25.0" prefWidth="170.0" text="Procesar noticias">
            <font>
               <Font name="System Bold" size="12.0" />
            </font></Button>
         <TableView fx:id="tableView" prefHeight="300.0" prefWidth="106.0">
            <columns>
               <TableColumn fx:id="titulo" prefWidth="88.0" text="TITULO" />
               <TableColumn fx:id="publicador" prefWidth="95.0" text="PUBLICADOR" />
               <TableColumn fx:id="fecha" prefWidth="91.0" text="FECHA" />
               <TableColumn fx:id="contenido" prefWidth="210.0" text="CONTENIDO" />
            </columns></TableView>
      </VBox>
      <Button fx:id="salir" layoutX="272.0" layoutY="475.0" mnemonicParsing="false" onAction="#atras" text="Salir">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
